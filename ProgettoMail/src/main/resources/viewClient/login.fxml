<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="300.0"
            prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="mailProject.client.LoginController">
   <ImageView fx:id="imageViewMail" fitHeight="138.0" fitWidth="150.0" layoutX="250.0" layoutY="105.0"
              nodeOrientation="INHERIT" preserveRatio="true">
   </ImageView>
   <TextField fx:id="mailaddress" layoutX="45.0" layoutY="240.0" prefHeight="34.0" prefWidth="440.0"
              promptText="Inserisci l'indirizzo E-Mail">
      <font>
         <Font name="Calibri" size="18.0"/>
      </font>
   </TextField>
   <Button fx:id="login" layoutX="500.0" layoutY="240.0" mnemonicParsing="false" onAction="#checkEmail" text="Login">
      <cursor>
         <Cursor fx:constant="HAND"/>
      </cursor>
      <font>
         <Font name="Calibri" size="18.0"/>
      </font>
   </Button>
   <!--<ImageView fx:id="imageViewWelcome" fitHeight="374.0" fitWidth="374.0" layoutX="113.0" layoutY="-134.0" pickOnBounds="true" preserveRatio="true">
   </ImageView>-->
   <Text id="welcomeText" layoutY="60.0" layoutX="90.0">
      <font>
         <Font name="Helvetica" size="26.0"/>
      </font>
      Benvenuto nel nostro servizio email
   </Text>
</AnchorPane>
